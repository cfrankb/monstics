        MENUITEM "Suprimer\tSuprr",             IDM_DELETE
        MENUITEM "Propriétés\tAlt-Entrée",      IDM_PROPERTIES
        MENUITEM SEPARATOR
        MENUITEM "Exécuter...",                 IDM_RUN
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "Test"
    BEGIN
        MENUITEM "Test",                        IDM_TEST
        MENUITEM "Test2",                       IDM_TEST2
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&Réorganisation automatique", IDM_OPTION_REORGANIZE_AUTOMATIC


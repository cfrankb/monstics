        MENUITEM SEPARATOR
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "&?"
    BEGIN
        MENUITEM "&Rubrique d'aide",            ID__RUBRIQUEDAIDE
        MENUITEM SEPARATOR
        MENUITEM "À &propos du Gestionaire de programmes", ID_APP_ABOUT
    END
END


